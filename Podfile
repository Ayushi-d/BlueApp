# Uncomment the next line to define a global platform for your project
platform :ios, '15.0'

target 'BLUE' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for BLUE
     pod 'Hero'
     pod 'FSCalendar'
     pod 'AGSwipeableTabBar'
     pod 'RateBar'
     pod 'IQKeyboardManagerSwift'
     pod 'XLPagerTabStrip', '9.0'
     pod 'Moya'
     # For Facebook Login
     # https://developers.facebook.com/docs/facebook-login/ios/?sdk=cocoapods
     pod 'FBSDKLoginKit','11.2.0'
     pod 'FBSDKCoreKit'
     pod 'FBSDKShareKit'

     # https://firebase.google.com/docs/auth/ios/google-signin
     # Pod Firebase Gogole Signin for gooogle login meeting sync
     pod 'Firebase/Auth'
     pod 'GoogleSignIn', '~> 5.0'
     pod 'Firebase/Core'
     pod 'Firebase/Messaging'
     
     #pod for ProgressBar
     #https://github.com/SVProgressHUD/SVProgressHUD
     pod 'SVProgressHUD', '2.2.5'
     
     #  Cosmos use for for display rating
     #https://github.com/evgenyneu/Cosmos
     pod 'Cosmos','23.0.0'
     
     # For displaying image and video preview
     # https://github.com/hyperoslo/Lightbox
     pod 'Lightbox', '2.5.0'
     
     pod 'OTPFieldView'
     pod 'CryptoSwift', '~> 1.6.0'

end
